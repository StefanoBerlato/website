---
import type { ServiceSection } from '@/types/sections/service-section.types';
import Divider from '@/web/components/divider.astro';
import SectionCard from '@/web/components/section-card.astro';
import Service from './service.astro';

export interface Props extends ServiceSection {}

const { config, service } = Astro.props;
---

<SectionCard {...config} isCollapsible>
  {service.flatMap((publication) => [<Service {...publication} />, <Divider />])}
</SectionCard>
