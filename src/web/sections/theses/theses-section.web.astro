---
import type { ThesesSection } from '@/types/sections/theses-section.types';
import Divider from '@/web/components/divider.astro';
import SectionCard from '@/web/components/section-card.astro';
import Theses from './theses.astro';

export interface Props extends ThesesSection {}

const { config, theses } = Astro.props;
---

<SectionCard {...config}>
  {theses.flatMap((publication) => [<Theses {...publication} />, <Divider />])}
</SectionCard>
