---
import type { TeachingSection } from '@/types/sections/teaching-section.types';
import Divider from '@/web/components/divider.astro';
import SectionCard from '@/web/components/section-card.astro';
import Teaching from './teaching.astro';

export interface Props extends TeachingSection {}

const { config, teaching } = Astro.props;
---

<SectionCard {...config}>
  {teaching.flatMap((teaching1) => [<Teaching {...teaching1} />, <Divider />])}
</SectionCard>
